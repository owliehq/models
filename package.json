{
  "name": "@owliehq/models",
  "version": "0.0.0-development",
  "description": "Organize & share your business logic ressources onto ODM style objects",
  "keywords": [],
  "main": "dist/models.umd.js",
  "types": "dist/models.d.ts",
  "module": "dist/models.es5.js",
  "typings": "dist/types/models.d.ts",
  "files": [
    "dist"
  ],
  "author": "Eric Scholl <eric@owlie.xyz>",
  "contributors": [
    "HEN Xavier <uminily@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/owliehq/models"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint --fix --ext .ts src/**/*.ts test/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage --runInBand",
    "test:nocoverage": "jest",
    "test:watch": "jest --coverage --watch --runInBand",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged",
    "npm:publish": "npm publish --access public",
    "prepush": "npm run test:prod && npm run build",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}",
      "src/**/*.{js,ts}"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.3",
    "@commitlint/config-conventional": "^7.6.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.17.9",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "colors": "^1.4.0",
    "commitizen": "^3.1.2",
    "core-js": "^3.4.8",
    "coveralls": "^3.0.9",
    "cross-env": "^5.2.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^1.0.1",
    "jest": "^25.0.0",
    "jest-config": "^25.0.0",
    "lint-staged": "^8.2.1",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.19.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.7.1",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "semantic-release": "^15.13.31",
    "shelljs": "^0.8.3",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "typedoc": "^0.15.4",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "p-iteration": "^1.1.8"
  }
}
